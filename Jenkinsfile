  agent any

  stages {

    stage("build") {
      
      steps {
        echo 'building the applinication...'
      }
    }

    stage("test"){

      steps {
        echo 'testing the applinication...'
      }
    }

    stage("deploy"){

      steps {
        echo 'deploying the applinication...'
      }
    }
  }
}
